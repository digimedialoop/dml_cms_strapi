{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Seiten (SEO)",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "pageName": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "navigation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::navigation.navigation",
      "mappedBy": "pageName"
    },
    "header_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "elements.seo"
    },
    "pageSections": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.standard-section",
        "sections.page-components"
      ]
    },
    "pageLink": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    }
  }
}
